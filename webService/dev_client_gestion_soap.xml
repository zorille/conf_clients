		<!-- parametre de l'objet creer_gestion_connexion_url -->
		<useSSL>oui/non</useSSL>
		<force_url>non</force_url>
		<tunnel>
			<host>hostname.domain</host>
			<port>10081</port>
			<timeoutssh>1200</timeoutssh>
		</tunnel>
		<proxy>
			<host>192.168.200.100</host>
			<port>443</port>
			<type>0</type><!-- PROXY_HTTP -->
